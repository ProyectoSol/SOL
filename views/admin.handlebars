<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap 101 Template</title>
    <script src="js/jquery-1.11.3.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js"></script>
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <script>
      $(document).ready(function(){
        $("#boton").click(function(){
          var usuario = $('#campousuario').val();
          $.get("/clientedatos/"+usuario ,{ user: usuario}, function(datosUsuario) {
            var user = JSON.stringify(datosUsuario);
            alert("Hola: " + user.usuario);
          });
        });
      });
    </script>
  </head>
  <body>
     <header class="jumbotron">
        <div class="container">
            <div class="row">
                <div>
                    <h1 align="center">Panel De Control</h1>
                </div>
            </div>
        </div>
    </header>
      <div class="container">
        <div class="row">
            <div>
              <ul class="nav nav-tabs">
                <li role="presentation" class="active"><a href="#">Usuario</a></li>
                <li role="presentation"><a href="#">Dispositivo</a></li>
                <li role="presentation"><a href="#">Estadisticas</a></li>
              </ul>
            </div>
        </div>
        <div class="row" style="padding:40px;">
          <div class="col-md-6">
          <form class="form-inline">
            <div class="form-group">
              <input type="email" class="form-control" id="campousuario" placeholder="Email">
            </div>
            <button type="button" id="boton" class="btn btn-default">Buscar</button>
          </form>
          </div>
          <div id="usus" class="col-md-6">           
          <ul>
            {{#each datos}}
            <li>{{usuario}}</li>
            {{/each}}
            </ul>
          </div>
          <div id="texto"></div>
        </div>
       </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
  </body>
</html>