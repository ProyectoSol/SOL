<body class="body">
  <div id="sol">
    <img src="images/sol.png"></img>
  </div>
  <div class="hamburgerMenu"><span></span></div>
  <div class="menu">

    <span><a href="#section1" class="sindeco">Home</a></span>
    <span><a href="/panel"  class="sindeco">Panel de control</a></span>
    <!--<span><a href="#sectionEstadisticas" class="sindeco">Estadísticas</a></span>-->
    <span><a href="/logout" class="sindeco">Cerrar sesión</a></span>


  </div>

  <div id="hero" class="w-section w-clearfix hero">
    <div id="Clouds">
      <div class="Cloud Foreground"></div>
      <div class="Cloud Background"></div>
      <div class="Cloud Foreground"></div>
      <div class="Cloud Background"></div>
      <div class="Cloud Foreground"></div>
      <div class="Cloud Background"></div>
      <div class="Cloud Background"></div>
      <div class="Cloud Foreground"></div>
      <div class="Cloud Background"></div>
      <div class="Cloud Background"></div>
    </div>

    <h4 class="lugar">{{city}}</h4>
    <h1 class="radio" id="nivelUV">{{Uv}}</h1><img height="100" id="banderaimg" src="/banderas/verde.png" class="bandera">
    <div class="divtiempo"><img src="/images/{{meteo}}.png">
      <h4 id="max">Max {{max}}º</h4>
      <h5 id="min">Min {{min}}º</h5>
    </div>

    <div id="box1">
      <div class=" content  alert {{style}}">
        <h2 class="titulorecomendacion">Recomendación</h2>
        <h4 class="segundotitulorecomendacion"> {{User}} utiliza una crema de protección</h4>
        <div id="fototipo" hidden> {{Fototipo}} </div>
        <div class="textrecomendacion">
          <p id="seRecomienda">Abre el menú y ve al panel para añadir tus datos</p>
        </div>
      </div>
    </div>

  </div>

  <div class="w-section sectionestadisticas">
    <div class="w-row estadisticas">
      <div class="  col-md-4 col-sm-3 col-xs-10 columnaestadistica1">
        <div class="divestadisticas">
          <!------------estadistica DIA-------------->
            <script src="/banderas/banderas.js"></script>
          <h3 class="tituloestadistica">Día</h3>
          <div id="HoraR" hidden>{{HoraR}}</div>
          <div id="RadiacionHora" hidden>{{RadiacionHora}}</div>

          <canvas id="mycanvas2" class="imagenestadistica">
            <script src="/estadisticasJS/Dia.js"></script>
          </canvas>

        </div>
      </div>
      <div class=" col-md-4 col-sm-3 col-xs-10 columnaestadistica2">
        <div class="divestadisticas">
          <h3 class="tituloestadistica">Semana</h3>
          <!--    <div>-->

          <div id="radioA" hidden>{{Esemana}}</div>
          <div id="SemanaDia" hidden>{{EsemanaDia}}</div>
          <canvas id="mycanvas" class="imagenestadistica">
            <script src="/estadisticasJS/Semana.js"></script>

          </canvas>

          <!-- </div>-->
        </div>
      </div>
      <div class=" col-md-4 col-sm-3 col-xs-10 columnaestadistica3">
        <div class="divestadisticas">
          <h3 class="tituloestadistica">Año</h3>
          <div id="RadiacionAnual" hidden>{{RadiacionAnual}}</div>
          <div id="fechaAnual" hidden>{{mesA}}</div>
          <script src="/estadisticasJS/Anual.js"></script>
          <canvas id="mycanvas3" class="imagenestadistica"></canvas>
        </div>
      </div>

    </div>
  </div>
  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script type="text/javascript" src="/js/webflow.js"></script>


